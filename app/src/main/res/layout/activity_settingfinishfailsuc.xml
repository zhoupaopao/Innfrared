<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android" >
    <include layout="@layout/layout_title_back" android:id="@+id/title_layout" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x20"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="@dimen/x52"
                    android:layout_height="@dimen/x52"
                    android:layout_gravity="center_horizontal"
                    android:src="@mipmap/finish" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/x14"
                    android:text="@string/setting_success" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center">

                    <TextView
                        android:id="@+id/tv_db1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="@dimen/x10"
                        android:text="电表 0025515155数据已上传" />

                    <TextView
                        android:id="@+id/data_jy"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/x16"
                        android:layout_toRightOf="@id/tv_db1"
                        android:textColor="@color/blue"
                        android:text="数据校验" />
                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/x18"
                android:background="@drawable/radius_yy"
                android:layout_margin="@dimen/x10">
                <TextView
                    android:id="@+id/fail_db"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="@dimen/x16"
                    android:text="电表 2221515151"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/ll1"
                    android:layout_marginTop="@dimen/x18"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:id="@+id/linearLayout">
                    <ImageView
                        android:layout_width="@dimen/x100"
                        android:layout_height="@dimen/x160"
                        android:layout_alignTop="@+id/linearLayout"
                        android:layout_centerHorizontal="true"
                        android:src="@mipmap/none_data" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:text="@string/none_data"/>
                    <TextView
                        android:layout_marginTop="@dimen/x5"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/none_data_msg"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/x60"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/re_scan"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/x30"
                        android:layout_weight="1"
                        android:textColor="@color/blue"
                        android:textSize="@dimen/x18"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="重新扫描"/>
                    <TextView
                        android:id="@+id/call"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/x30"
                        android:layout_weight="1"
                        android:textColor="@color/blue"
                        android:textSize="@dimen/x18"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="联系维护"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>

